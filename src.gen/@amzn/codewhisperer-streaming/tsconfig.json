{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "incremental": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "useUnknownInCatchVariables": false,
    "composite": true
  },
  "exclude": ["test/", "dist-*", "node_modules"]
}
